{
  "version": 2,
  "buildCommand": "npm run build:production",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "GENERATE_SOURCEMAP": "false"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
